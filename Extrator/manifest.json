{
  "manifest_version": 3,
  "name": "Extrator de CNPJ",
  "description": "Extensão para exportar informações da api a casa dos dados.",
  "version": "1.0",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "informativo.html",
    "default_icon": "home.png"
  }

}
